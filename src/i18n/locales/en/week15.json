    "leakPatterns": [
      {
        "pattern": "Event Listener not removed",
        "code": "useEffect(() => { window.addEventListener(...); }, []);",
        "fix": "removeEventListener in cleanup function"
      },
      {
        "pattern": "setInterval not cleared",
        "code": "setInterval(() => { ... }, 1000);",
        "fix": "clearInterval in useEffect cleanup"
      },
      {
        "pattern": "Closure retains DOM reference",
        "code": "const el = document.getElementById(...);",
        "fix": "Assign null when not needed"
      },
      {
        "pattern": "Subscription not unsubscribed",
        "code": "observable.subscribe(...);",
        "fix": "unsubscribe in cleanup"
      }
    ]
  },
